{
  "v": "4.0",
  "workspace": {
    "name": "revel",
    "defaultEnv": "revel",
    "environments": [
      {
        "name": "revel",
        "recipe": null,
        "machineConfigs": [
          {
            "name": "ws-machine",
            "limits": {
              "ram": 1024
            },
            "source": {
              "type": "dockerfile",
              "location": "https://raw.githubusercontent.com/rickspencer3/revel-dev-container/che/.codenvy.dockerfile"
            },
            "type": "docker",
            "dev": true
          }
        ]
      }
    ],
    "projects": [
      {
        "name": "revel-sample",
        "type": "blank",
        "path": "/revel-sample",
        "source": {
          "type": "git",
          "location": "https://github.com/rickspencer3/revel-dev-container.git",
          "parameters": {
            "branch": "che",
            "keepVcs": "true"
          }
        },
        "mixins": [
          "git",
          "pullrequest"
        ],
        "attributes": {
          "local_branch": [ "contrib" ],
          "contribute_to_branch" : [ "master" ]
        },
        "modules": [],
        "problems": []
      }
    ],
    "commands": [
    ]
  },
  "ide": {
    "onProjectsLoaded": {
      "actions": [
    }
  }
